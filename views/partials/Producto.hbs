<script>
  /**
Busco los productos que se me pidan
**/
    $(document).ready(function () {

        $("#buscar").click(function () {
            buscar();
        });
        function buscar() {
            var criterio = $("#criterio").val();
            var texto = $("#texto").val();
            var url = "/buscarProducto";
            $.ajax({
                url: url,
                data: 'criterio=' + criterio + '&texto=' + texto,
                success: function (data, textStatus, jqXHR) {
                    console.log(data);
                    var html = '';
                    $.each(data, function (index, item) {

                        html += '<tr>';
                        html += '<td>' + item.nombre + '</td>';
                        html += '<td>' + item.categoria + '</td>';
                        html += '<td>' + item.precio + '</td>';
                        html += '<td>' + item.descripcion + '</td>';
                        var m = item.descripcion.split(" ");
                        var palabra = "";
                        for (var i = 0; i < m.length; i++) {
                            palabra += m[i];
                        }
                        html += '<td>' + "0" + '</td>';
                        html += '<td><a  class="btn btn-default" data-toggle="modal" onclick=$("#idProducto").val("' + item.id + '"),$("#precio2").val("' + item.precio + '"),$("#nombre2").val("' + item.nombre + '"),$("#categoria2").val("' + item.categoria + '"),$("#descripcion2").val("' + palabra + '"),$("#stock2").val("0")  data-target="#modal2"><input type=image src="data:image/png;base64," width="25" height="25" style="padding-top: 5px">  Editar</a>';
                        html += '</tr>';
                    });
                    $("#tabla tbody").html(html);
                },
                error: function (jqXHR, textStatus, errorThrown) {
                    console.log(jqXHR.responseText);
                }
            });
        }


    });
</script>


